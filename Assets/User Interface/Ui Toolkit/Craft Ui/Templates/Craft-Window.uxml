            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="Recipes-Container" class="recipes-container" style="height: 400px;">
            <ui:Label tabindex="-1" text="Recipes" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Recipes-TItle" class="recipes-title" />
            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="Recipes" class="recipes">
                <ui:VisualElement name="Recipe" class="recipe" />
            </ui:ScrollView>
        </ui:VisualElement>
    </ui:VisualElement>
